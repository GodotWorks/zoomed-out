onready var anim = $AnimationPlayer

func initialize():
	$AnimationPlayer.play(\"appear\")


func _on_Quit_pressed():
	anim.play(\"Disappear\")
	get_tree().quit()


func _on_BackToMenu_pressed():
	anim.play(\"Disappear\")
	get_tree().paused = false
	get_tree().change_scene(\"res://screens/Menu.tscn\")
	
"

